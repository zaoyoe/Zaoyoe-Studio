{
    "buildCommand": null,
    "outputDirectory": ".",
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/api/:match*",
            "destination": "https://q6nh03pq.api.lncldglobal.com/:match*"
        },
        {
            "source": "/(.*)",
            "destination": "/$1.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                },
                {
                    "key": "Cache-Control",
                    "value": "no-cache, no-store, must-revalidate, max-age=0"
                }
            ]
        }
    ]
}